{
  "name": "pfinal/justauth-php",
  "description": "library of third-party authorized login",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "pfinal南丞",
      "email": "lampxiezi@163.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "symfony/http-foundation": "^5.0",
    "guzzlehttp/guzzle": "~6.0|~7.0",
    "ext-json": "*",
    "symfony/psr-http-message-bridge": "^2.0",
    "ext-openssl": "*",
    "nancheng/pfinal-request": "v0.13",
    "nancheng/pfinal-enum": "v0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "JustAuth\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "JustAuth\\Tests\\": "test/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
